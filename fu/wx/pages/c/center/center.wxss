/* pages/a/orderList/orderList.wxss */
.wrap{background-color:#f5f5f5;}

.bg{height:144rpx;padding:60rpx 30rpx;font-size:24rpx;color:#fff;background:linear-gradient(45deg,#e8645f 0%,#fa9b7a 100%) no-repeat;background-size:100% auto;overflow:hidden;}
.bg .img{width:144rpx;height:144rpx;margin-right:40rpx;display:inline-block;vertical-align:middle;border-radius:50%;}
.bg .user{display:inline-block;vertical-align:middle;}
.bg .name{font-size:32rpx;}
.bg .mobile{display:block;margin-top:14rpx;}
.bg .user .ico{width:100rpx;height:30rpx;margin-left:20rpx;vertical-align:middle;}
.bg .ico-setx{margin-top:18rpx;}

.fu-block{margin-bottom:20rpx;padding:0 30rpx;}
.fu-item{padding:0;}
.fu-item:nth-child(n+2){border-top:1rpx solid #e2e2e2}

.larger{line-height:110rpx;}


.bg{background-image:url(data:image/png;base64,)}

.ico-verity{background-image:url(data:image/png;base64,)}

.ico-unverity{background-image:url(data:image/png;base64,)}